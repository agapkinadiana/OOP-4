<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfApp1.themes">
    <Style x:Key="MenuTheme" TargetType="Menu">
        <Setter Property="Background" Value="#FFECC4C2" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="MenuItemTheme" TargetType="MenuItem">
        <Setter Property="Background" Value="#FFECC4C2" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="LabelTheme" TargetType="Label">
        <Setter Property="Background" Value="#FFECC4C2" />
    </Style>
    <Style x:Key="ComboBoxTheme" TargetType="ComboBox">
        <Setter Property="Background" Value="#FFECC4C2" />
    </Style>
    <Style x:Key="ToolBarTrayTheme" TargetType="ToolBarTray">
        <Setter Property="Background" Value="#FFECC4C2" />
    </Style>
    <Style x:Key="WrapPanelTheme" TargetType="WrapPanel">
        <Setter Property="Background" Value="#FFECC4C2" />
    </Style>
    <Style x:Key="RichTextBoxTheme" TargetType="RichTextBox">
        <Setter Property="Background" Value="White" />
    </Style>
</ResourceDictionary>